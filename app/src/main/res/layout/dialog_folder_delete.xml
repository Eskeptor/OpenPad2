<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dialog_folder_margin"
        android:layout_marginRight="@dimen/dialog_folder_margin"
        android:layout_marginTop="@dimen/dialog_folder_margin"
        android:text="@string/folder_dialog_delete_com1"
        android:textColor="@color/black"
        android:textSize="@dimen/dialog_folder_text_size" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dialog_folder_margin"
        android:layout_marginLeft="@dimen/dialog_folder_margin"
        android:layout_marginRight="@dimen/dialog_folder_margin"
        android:text="@string/folder_dialog_delete_com2"
        android:textColor="@color/defaultColorAccent" />
</LinearLayout>